
long long gcd(long long a, long long b) {
    while (b != 0) {
        long long temp = b;
        b = a % b;
        a = temp;
    }
    return a;
}



// LCM using custom GCD
long long lcm(long long a, long long b) {
    return (a / gcd(a, b)) * b;
}



// LCM of all digits of the number
long long lcm_of_digits(long long num) {
    string digits = to_string(num);
    long long result = 1;

    for (char ch : digits) {
        int digit = ch - '0';
        if (digit == 0) continue; // skip 0 to avoid LCM issues
        result = lcm(result, digit);
    }

    return result;
}
